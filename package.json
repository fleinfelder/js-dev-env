{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "author": "Florian Leinfelder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fleinfelder/js-dev-env.git"
  },
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.*  src  buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "lint:report": "npm run lint -- --format junit",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "security-check": "nsp check",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "dist": "babel-node buildScipts/distServer.js"
  },
  "dependencies": {
    "whatwg-fetch": "2.0.3",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.4",
    "react-router": "4.1.1",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.24.1",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-jest": "19.0.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-watch": "3.1.0",
    "extract-text-webpack-plugin": "2.1.0",
    "express": "4.15.2",
    "html-webpack-plugin": "2.28.0",
    "jest": "19.0.2",
    "jsdom": "10.0.0",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "style-loader": "0.16.1",
    "open": "0.0.5",
    "rimraf": "2.6.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
